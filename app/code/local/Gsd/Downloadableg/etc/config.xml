<?xml version="1.0"?>
<config>
    <modules>
        <Gsd_Downloadableg>
            <version>0.1.0</version>
        </Gsd_Downloadableg>
    </modules>
    <global>
        <models>
            <downloadableg>
                <class>Gsd_Downloadableg_Model</class>
            </downloadableg>
        </models>
        <blocks>
            <downloadableg>
                <class>Gsd_Downloadableg_Block</class>
            </downloadableg>
        </blocks>
        <helpers>
            <downloadableg>
                <class>Gsd_Downloadableg_Helper</class>
            </downloadableg>
        </helpers>
    </global>
    <frontend>
        <routers>
            <downloadableg>
                <use>standard</use>
                <args>
                    <module>Gsd_Downloadableg</module>
                    <frontName>downloadableg</frontName>
                </args>
            </downloadableg>
        </routers>
    </frontend>
</config>