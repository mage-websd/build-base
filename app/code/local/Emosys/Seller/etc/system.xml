<config>
    <tabs>
        <seller translate="label" module="seller">
            <label>Seller Config</label>
            <sort_order>150</sort_order>
        </seller>
    </tabs>
    <sections>
        <seller>
            <label>Seller</label>
            <tab>seller</tab>
            <frontend_type>text</frontend_type>
            <sort_order>100</sort_order>
            <show_in_default>1</show_in_default>
            <show_in_website>1</show_in_website>
            <show_in_store>1</show_in_store>
            <groups>
                <general>
                    <label>General Config</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>10</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <seller_approval translate="label">
                            <label>Seller Approval Required?</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>10</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </seller_approval>
                        <product_approval translate="label">
                            <label>Product Approval Required?</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>20</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </product_approval>
                    </fields>
                </general>
                <product>
                    <label>Product Config</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>50</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <allowed_types translate="label">
                            <label>Allowed Product Types</label>
                            <frontend_type>multiselect</frontend_type>
                            <source_model>seller/system_config_source_product</source_model>
                            <sort_order>0</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </allowed_types>
                        <allowed_attribute_set translate="label">
                            <label>Allowed Attribute Set</label>
                            <frontend_type>multiselect</frontend_type>
                            <source_model>seller/system_config_source_set</source_model>
                            <sort_order>10</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </allowed_attribute_set>
                        <max_upload_image translate="label">
                            <label>Maximum Number of Images to Upload</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>30</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </max_upload_image>
                    </fields>
                </product>
            </groups>
        </seller>
    </sections>
</config>