<layout>
    <default>
        <reference name="head">
            <action method="addJs" ifconfig="baseg/js/jquery_enable">
                <script>jquery/jquery-1.11.1.min.js</script>
            </action>
            <action method="addJs" ifconfig="baseg/js/jquery_enable">
                <script>jquery/jquery-noconflict.js</script>
            </action>
            <action method="addItem">
                <type>skin_css</type>
                <file>bootstrap-3.3.5/css/bootstrap.min.css</file>
            </action>
            <action method="addItem">
                <type>skin_js</type>
                <file>bootstrap-3.3.5/js/bootstrap.min.js</file>
            </action>
            <action method="addItem">
                <type>skin_css</type>
                <name>css/allpage.css</name>
            </action>
            <action method="addItem">
                <type>skin_css</type>
                <name>css/allpage_responsive.css</name>
            </action>
            <action method="addItem">
                <type>skin_js</type>
                <name>js/allpage.js</name>
            </action>
            <action method="addJs">
                <script>tiny_mce/tiny_mce.js</script>
            </action>
        </reference>
        <reference name="content">
            <block type="core/template" name="data_base_url" template="baseg/base_url.phtml" before="-"/>
            <block type="core/template" name="ajax_loading" template="baseg/ajax_loading.phtml" before="-"/>
        </reference>
        <!--<remove name="catalog.topnav" />-->
        <reference name="header">
            <block type="page/html_topmenu" name="catalog_leftmenu_mobile" template="page/html/leftmenu_mobile.phtml"/>
        </reference>
    </default>
</layout>